VITE_DATABASE_HOST=your-rds-endpoint.rds.amazonaws.com
VITE_DATABASE_PORT=5432
VITE_DATABASE_NAME=phaserai_dev
VITE_DATABASE_USER=phaserai_admin
VITE_DATABASE_PASSWORD=your_database_password

