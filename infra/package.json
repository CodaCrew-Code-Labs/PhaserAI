{
  "name": "phaserai-infra",
  "version": "1.0.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy --all",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "migrate:up": "node lib/migration-runner.js up",
    "migrate:down": "node lib/migration-runner.js down",
    "migrate:status": "node lib/migration-runner.js status",
    "migrate:list": "node lib/migration-runner.js list",
    "migrate:prod": "NODE_ENV=production node lib/migration-runner.js up"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "aws-cdk": "^2.170.0",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.490.0",
    "aws-cdk-lib": "^2.170.0",
    "constructs": "^10.3.0",
    "pg": "^8.11.3"
  }
}
