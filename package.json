{
  "name": "shadcnui",
  "type": "module",
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --quiet ./src",
    "lint:fix": "eslint --fix ./src",
    "lint:report": "eslint --format json --output-file eslint-report.json ./src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "echo \"No tests specified yet\"",
    "ci:build": "npm run type-check && npm run lint && npm run build",
    "ci:format-check": "npm run format:check",
    "ci:security-audit": "npm audit --audit-level moderate",
    "fix-lockfile": "./scripts/fix-lockfile.sh",
    "verify-setup": "./scripts/verify-setup.sh",
    "docker:build": "docker build -t phaserai:latest --build-arg VITE_SUPABASE_URL=https://dummy-url.supabase.co --build-arg VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.dummy-key .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t phaserai-dev:latest --build-arg VITE_SUPABASE_URL=https://dummy-url.supabase.co --build-arg VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.dummy-key .",
    "docker:run": "docker run -p 8080:80 phaserai:latest",
    "docker:run-dev": "docker run -p 5173:5173 phaserai-dev:latest",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=phaserai:latest) || true",
    "docker:clean": "docker rmi phaserai:latest phaserai-dev:latest || true",
    "docker:logs": "docker logs $(docker ps -q --filter ancestor=phaserai:latest)",
    "docker:shell": "docker exec -it $(docker ps -q --filter ancestor=phaserai:latest) /bin/sh"
  },
